	file = {Ságodi et al. - 2024 - Reality Check Assessing GPT-4 in Fixing Real-Worl.pdf:/home/yiannis/Zotero/storage/DNA4D8WZ/Ságodi et al. - 2024 - Reality Check Assessing GPT-4 in Fixing Real-Worl.pdf:application/pdf},
